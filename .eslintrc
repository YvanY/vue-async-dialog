{
  "root": true,
  "extends": [
    "plugin:vue/essential",
    "standard"
  ],
  "rules": {
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "always"
    }],
    "operator-linebreak": ["error", "before"]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
